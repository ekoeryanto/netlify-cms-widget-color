{
  "name": "netlify-cms-widget-native-color",
  "version": "0.1.1",
  "description": "Native Color widget for Netlify CMS.",
  "main": "dist/cjs/main.js",
  "module": "dist/esm/index.js",
  "browser": "dist/main.js",
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:esm": "babel src -d dist/esm",
    "build:browser": "webpack --display errors-only --mode=production",
    "build:umd": "webpack --display errors-only --mode=production --library-target umd2",
    "build:common": "webpack --display errors-only --mode=production --library-target commonjs2",
    "watch": "webpack -w src --mode=development",
    "prepublishOnly": "npm-run-all clean build"
  },
  "keywords": [
    "netlify",
    "netlify-cms",
    "cms",
    "widget",
    "native-color"
  ],
  "author": "Eko Eryanto <ekoeryanto@gmail.com>",
  "bugs": {
    "url": "https://github.com/ekoeryanto/netlify-cms-widgets/issues"
  },
  "homepage": "https://github.com/ekoeryanto/netlify-cms-widgets/tree/master/packages/native-color#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekoeryanto/netlify-cms-widgets.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  },
  "peerDependencies": {
    "create-react-class": "^15.6.0",
    "immutable": "^3.8.0",
    "netlify-cms": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-immutable-proptypes": "^2.1.0"
  }
}
